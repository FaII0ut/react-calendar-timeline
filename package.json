    "react": "^18.1.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.1.1",
    "react-testing-library": "^8.0.1",
    "rimraf": "^3.0.2",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.13.0",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  }
}
